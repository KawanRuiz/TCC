<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ProdutosServlet</servlet-name>
        <servlet-class>Control.ProdutosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LotesServlet</servlet-name>
        <servlet-class>Control.LotesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ComandasServlet</servlet-name>
        <servlet-class>Control.ComandasServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuariosServlet</servlet-name>
        <servlet-class>Control.UsuariosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DevedoresServlet</servlet-name>
        <servlet-class>Control.DevedoresServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RelatoriosServlet</servlet-name>
        <servlet-class>Control.RelatoriosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>Control.Upload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProdutosServlet</servlet-name>
        <url-pattern>/ProdutosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LotesServlet</servlet-name>
        <url-pattern>/LotesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ComandasServlet</servlet-name>
        <url-pattern>/ComandasServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuariosServlet</servlet-name>
        <url-pattern>/UsuariosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DevedoresServlet</servlet-name>
        <url-pattern>/DevedoresServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RelatoriosServlet</servlet-name>
        <url-pattern>/RelatoriosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/Upload</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
